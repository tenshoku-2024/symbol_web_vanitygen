{
	"compilerOptions":{
		"declaration":true,
		"esModuleInterop":true,
		"module":"esnext",
		"moduleResolution":"node",
		"outDir":"./dist",
		"rootDir":"./src",
		"skipLibCheck":true,
		"strict":true,
		"target":"esnext"
	},
	"exclude":[
		"./src/**/*.test.ts"
	],
	"include":[
		"./src/**/*.ts"
	]
}
